{
  "name": "api-campaigns",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:dev:start": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "docker:e2e:start": "docker-compose -f docker-compose.e2e.yml up -d",
    "docker:e2e:stop": "docker-compose -f docker-compose.e2e.yml down",
    "lint": "eslint \"{src,apps,libs,test,util}/**/*.ts\" --fix",
    "lint:staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:ci": "yarn docker:e2e:start && jest --config ./test/jest-e2e.json && yarn docker:e2e:stop",
    "test:all": "yarn test && yarn test:e2e",
    "get-token": "util/scripts/getToken.js",
    "commitlint": "commitlint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cerbero/mod-auth": "^0.6.1",
    "@elastic/ecs-helpers": "^1.1.0",
    "@fastify/static": "^6.4.0",
    "@google-cloud/storage": "^6.3.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.0.3",
    "@nestjs/swagger": "^6.0.4",
    "axios": "^0.27.2",
    "chance": "^1.1.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify-multer": "^2.0.3",
    "fastify-multipart": "^5.4.0",
    "mongoose": "^6.4.4",
    "multer": "^1.4.5-lts.1",
    "nestjs-pino": "^2.6.0",
    "node-fetch": "^3.2.8",
    "pino-http": "^8.1.1",
    "prompts": "^2.4.2",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.30.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@beamery/commitlint-config": "^1.3.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/jest-sinon": "^1.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/ramda": "^0.28.15",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "form-data": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "28.1.2",
    "jest-sinon": "^1.0.4",
    "lint-staged": "^13.0.3",
    "mongodb-memory-server": "^8.7.2",
    "node-fetch-cookies": "^2.0.4",
    "prettier": "^2.3.2",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5",
    "webpack-cli": "^4.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules/(?!@cerbero)"
    ]
  }
}
